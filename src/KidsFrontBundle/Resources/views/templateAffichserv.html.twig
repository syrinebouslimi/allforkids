<script>
    $(document).ready(function() {

        const prev = document.querySelector('.demo-prev');
        const next = document.querySelector('.demo-next');
        const picked = document.querySelector('.demo-picked');
        const last = document.querySelector('.demo-last');
        var pickedDate;

        function updateInfo() {

            if (this.lastSelectedDay) {

                picked.innerHTML = '';
                for (days of this.selectedDays) {
                    var li = document.createElement('li');
                    li.innerHTML = days;
                    pickedDate = days;
                    picked.appendChild(li);
                }
            }
        }

        const myCalendar = new HelloWeek({
            selector: '.hello-week',
            lang: 'en',
            langFolder: '../../hello-week-master/dist/langs/',
            format: 'DD-MM-YYYY',
            weekShort: true,
            monthShort: false,
            disablePastDays: true,
            disabledDaysOfWeek: "0",
            multiplePick: false,
            minDate: false,
            maxDate: false,
            weekStart: 1,
            onLoad: updateInfo,
            onChange: updateInfo,
            onSelect: updateInfo
        });

        prev.addEventListener('click', () => myCalendar.prev());

        next.addEventListener('click', () => myCalendar.next());

        $('#horaire').timeselect({
            'step': 30,
            'format': 'HH:mm',
            autocompleteSettings: {
                autoFocus: true
            }
        });

        $("#savebotom").click(function () {
            alert('rrrrr');
            var title = $("#nomprenom").val();

            var dataString = {};
            dataString['nomprenom'] = title;

            dataString['daterendezvous'] = pickedDate.toString()
            dataString['horaire'] =$('#horaire').val();

            dataString['service'] =$('#idService').val();
            $.ajax({
                method: 'POST',
                dataType: 'json',
                url: '{{ path('ajoutrendezvous') }}',
                data: dataString,
                success: function (data) {
                    $('#modal-default').modal('hide');
                    $('#modal-success').modal('show');
                    $('#success-rendezvous').text('Rendez-vous confirmé.');
                    $("#nomprenom-picked").text(dataString['nomprenom']);
                    $("#daterendezvous-picked").text(dataString['daterendezvous']);
                    $("#horaire-picked").text(dataString['horaire']);
                    $("#nomprenom").val("");
                    $(".demo-picked").empty();
                    $(".demo-picked").append('<span class="label label-success">n/a</span>')
                    $("#horaire").val("");
                }
            });

        });
    });
</script>

